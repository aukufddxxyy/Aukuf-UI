{
  "name": "aukuf-ui",
  "version": "1.0.0",
  "description": "Components library for Aukuf",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/index.css"
  ],
  "main": "./dist/umd/aukuf-ui.umd.cjs",
  "module": "./dist/es/aukuf-ui.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/aukuf-ui.js",
      "require": "./dist/umd/aukuf-ui.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "build": "run-s build-only move-style",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css"
  },
  "keywords": [],
  "author": "Aukuf",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.66",
    "@iconify/vue": "^4.1.2",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  }
}
